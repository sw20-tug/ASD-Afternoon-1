                    <TableColumn fx:id="vocabularyNameColumn" prefWidth="210" text="Name"/>
                    <TableColumn fx:id="vocabularyTranslation" prefWidth="70" text="Language"/>
                    <TableColumn fx:id="checkBoxColumn" prefWidth="70" sortable="false" text="Selection"/>
                </columns>
            </TableView>

            <Label fx:id="user_info" layoutX="67.0" layoutY="491.0" prefHeight="62.0" prefWidth="315.0"
