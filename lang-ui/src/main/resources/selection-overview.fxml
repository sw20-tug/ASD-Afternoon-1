    <AnchorPane prefHeight="602.0" prefWidth="800.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0"
                AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <children>

            <Label fx:id="title" alignment="CENTER" layoutX="50.0" layoutY="73.0" maxWidth="-Infinity" prefHeight="42.0"
                   prefWidth="350" text="Select Your Groups" textAlignment="CENTER">
                <font>
                    <Font name="Abyssinica SIL" size="36.0"/>
                </font>
            </Label>

            <Button fx:id="return_btn" layoutX="50.0" layoutY="31.0" prefHeight="16.0" prefWidth="70.0" text="Return"
                    GridPane.rowIndex="8">
                <font>
                    <Font name="Abyssinica SIL" size="14.0"/>
                </font>
            </Button>
            <Line endX="171.1016387939453" endY="-0.8284531831741333" layoutX="232.0" layoutY="128.0"
                  startX="-180.19125366210938" startY="-0.8284209966659546" strokeMiterLimit="20.0"/>


            <Button fx:id="train_btn" layoutX="50.0" layoutY="456.0" mnemonicParsing="false" text="Train">
                <font>
                    <Font name="Abyssinica SIL" size="14.0"/>
                </font>
            </Button>

            <Button fx:id="study_btn" layoutX="348.0" layoutY="456.0" mnemonicParsing="false" text="Study">
                <font>
                    <Font name="Abyssinica SIL" size="14.0"/>
                </font>
            </Button>

            <TableView fx:id="vocab_list" layoutX="51.0" layoutY="151.0" prefHeight="293.0" prefWidth="353.0"
